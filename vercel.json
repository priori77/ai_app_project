{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/package.json"
      }
    ],
    "functions": {
      "api/**/index.py": {
        "runtime": "python3.11"
      }
    }
  }
  